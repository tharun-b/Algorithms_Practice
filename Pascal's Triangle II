class Solution {
public:
    vector<int> getRow(int rowIndex) {
        if(rowIndex <1) return vector<int>(1,1);
        
        vector<int> ret(rowIndex+1,0);
        long int prev =1; 
        ret[0] = ret[rowIndex] = 1; 
        
        for(int i = 1 ; i<=((rowIndex+1)/2); i++ )
        {
            prev = (prev*(rowIndex-i+1))/(i);
            ret[i] = prev;
            ret[rowIndex-i] = prev;
        }
        
        return ret;
        
    }
};
