class Solution {
public:
    int findLUSlength(string a, string b) {
        
        int n = a.size(); 
        int m = b.size(); 
        
        int ma = max(n,m);
        
        if(ma ==0) return -1; 
        if(m == n ) 
        {
            if(a.compare(b) == 0)
                return -1; 
        }
        
       return ma; 
        
    }
};
